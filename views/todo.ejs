<!DOCTYPE html>
<html>
  <head>
    <title>Todo List</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= greeting %></h1>
    <ul>
      <% tasks.forEach(function(task){ %>
      <li><%= task.title %></li>
      <li><%= task.due_date %></li>
      <li><%= task.description %></li>
      <li><%= task.priority %></li>
      <%  }) %>
    </ul>

    <p class="task_title"><%=  %></p>
  </body>
</html>